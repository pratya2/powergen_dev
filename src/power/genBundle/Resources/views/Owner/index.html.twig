<h1>Owner list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Name</th>
            <th>Registerdate</th>
            <th>Registernumber</th>
            <th>Birthday</th>
            <th>Nationality</th>
            <th>Idcard</th>
            <th>Districtofissue</th>
            <th>Proviceofissue</th>
            <th>Adressnumber</th>
            <th>Lane</th>
            <th>Street</th>
            <th>Villagenumber</th>
            <th>Subdistrict</th>
            <th>District</th>
            <th>Province</th>
            <th>Poscode</th>
            <th>Telnumber</th>
            <th>Faxnumber</th>
            <th>Iscorp</th>
            <th>Mobilephone</th>
            <th>Website</th>
            <th>Email</th>
            <th>Id</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('owner_show', { 'id': entity.id }) }}">{{ entity.name }}</a></td>
            <td>{% if entity.registerDate %}{{ entity.registerDate|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.registerNumber }}</td>
            <td>{% if entity.birthDay %}{{ entity.birthDay|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.nationality }}</td>
            <td>{{ entity.idCard }}</td>
            <td>{{ entity.districtOfIssue }}</td>
            <td>{{ entity.proviceOfIssue }}</td>
            <td>{{ entity.adressNumber }}</td>
            <td>{{ entity.lane }}</td>
            <td>{{ entity.street }}</td>
            <td>{{ entity.villageNumber }}</td>
            <td>{{ entity.subDistrict }}</td>
            <td>{{ entity.district }}</td>
            <td>{{ entity.province }}</td>
            <td>{{ entity.poscode }}</td>
            <td>{{ entity.telNumber }}</td>
            <td>{{ entity.faxNumber }}</td>
            <td>{{ entity.iscorp }}</td>
            <td>{{ entity.mobilePhone }}</td>
            <td>{{ entity.website }}</td>
            <td>{{ entity.email }}</td>
            <td>{{ entity.id }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('owner_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('owner_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('owner_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
