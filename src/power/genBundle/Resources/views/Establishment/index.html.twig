<h1>Establishment list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Name</th>
            <th>Address</th>
            <th>Lane</th>
            <th>Street</th>
            <th>Village</th>
            <th>Subdistrict</th>
            <th>District</th>
            <th>Province</th>
            <th>Region</th>
            <th>Poscode</th>
            <th>Telnumber</th>
            <th>Faxnumber</th>
            <th>Lat</th>
            <th>Lng</th>
            <th>Location</th>
            <th>Idnumber</th>
            <th>Id</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('establishment_show', { 'id': entity.id }) }}">{{ entity.name }}</a></td>
            <td>{{ entity.address }}</td>
            <td>{{ entity.lane }}</td>
            <td>{{ entity.street }}</td>
            <td>{{ entity.village }}</td>
            <td>{{ entity.subdistrict }}</td>
            <td>{{ entity.district }}</td>
            <td>{{ entity.province }}</td>
            <td>{{ entity.region }}</td>
            <td>{{ entity.poscode }}</td>
            <td>{{ entity.telNumber }}</td>
            <td>{{ entity.faxNumber }}</td>
            <td>{{ entity.lat }}</td>
            <td>{{ entity.lng }}</td>
            <td>{{ entity.location }}</td>
            <td>{{ entity.idnumber }}</td>
            <td>{{ entity.id }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('establishment_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('establishment_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('establishment_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
